		"prelude-ls": "*",

		"oop": "git+https://github.com/ymeine/oop",

		"http-server": "git+https://github.com/Kykstudio/http-server",
		"winston": "*",

		"pegjs": "git+https://github.com/dmajda/pegjs",
		"escape-html": "*",
		"performance-now": "*",
		"ariatemplates": "*"
	}
